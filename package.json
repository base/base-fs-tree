{
  "name": "base-fs-tree",
  "description": "Base plugin for creating file trees using archy. Requires the base-fs plugin, but can also be used as a gulp plugin.",
  "version": "0.2.6",
  "homepage": "https://github.com/node-base/base-fs-tree",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "node-base/base-fs-tree",
  "bugs": {
    "url": "https://github.com/node-base/base-fs-tree/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "LICENSE",
    "README.md",
    "utils.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "archy": "^1.0.0",
    "clone-deep": "^0.2.4",
    "extend-shallow": "^2.0.1",
    "file-contents": "^0.3.2",
    "is-valid-app": "^0.2.0",
    "isobject": "^2.1.0",
    "lazy-cache": "^2.0.1",
    "through2": "^2.0.1",
    "trim-leading-lines": "^0.1.1",
    "vinyl": "^1.2.0",
    "write": "^0.3.1"
  },
  "devDependencies": {
    "assemble": "^0.16.1",
    "assemble-handle": "^0.1.3",
    "delete": "^0.3.2",
    "gulp": "^3.9.1",
    "gulp-format-md": "^0.1.9",
    "gulp-gitignore": "^0.1.0",
    "mocha": "^2.5.3",
    "vinyl-fs": "^2.4.3",
    "yargs-parser": "^2.4.1"
  },
  "keywords": [
    "api",
    "app",
    "application",
    "archy",
    "base",
    "baseplugin",
    "building-blocks",
    "create",
    "file",
    "files",
    "framework",
    "fs",
    "plugin",
    "plugins",
    "tool",
    "toolkit",
    "tools",
    "tree"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "base-fs",
        "base-fs-conflicts",
        "base-task"
      ]
    },
    "reflinks": [
      "assemble",
      "base",
      "base-fs",
      "generate",
      "gulp",
      "update",
      "verb",
      "verb-generate-readme"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
